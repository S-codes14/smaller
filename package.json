{
  "name": "smaller-sites",
  "description": "Drag and drop website builder javascript library",
  "version": "1.0.0",
  "author": "Sibongumusa Lungelo <smlmnguni14@gmail.com>",
  "license": "Apache-2.0",
  "homepage": "http://smaller-sites.netlify.app",
  "main": "./main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/S-codes14/smaller-sites.git"
  },
  "dependencies": {
    "jquery": "^3.6",
    "nconf": "^0.7.2",
    "path": "^0.12.7",
    "url": "^0.11.0"
  },
  "keywords": [
    "smallerjs",
    "drag-drop",
    "drag&drop",
    "drag",
    "drop",
    "wysiwyg",
    "web",
    "template",
    "editor",
    "site",
    "builder"
  ],
  "scripts": {
    "start": "electron .",
    "gulp": "gulp",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "electron-packager ./ smaller-sites --all --out ~/Documents/SmallerSites/sites --version 0.30.8 --overwrite --icon=./app/img/app-icon.icns",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=app/img/app-icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . smaller-sites --overwrite --asar=true --platform=win32 --arch=ia32 --icon=app/img/app-icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Smaller sites\"",
    "package-linux": "electron-packager . smaller-sites --overwrite --asar=true --platform=linux --arch=x64 --icon=smaller.png --prune=true --out=release-builds"
  },
  "build": {
    "appId": "app.netlify.smaller-sites",
    "category": "public.app-category.productivity",
    "copyright": "Â©2021 Smaller Studioz",
    "dmg": {
      "icon": "app/img/app-icon.icns",
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "app/img/app-icon.ico"
    }
  },
  "devDependencies": {
    "electron": "^13.1.5",
    "electron-builder": "^22.11.7",
    "electron-packager": "^5.2.0",
    "electron-prebuilt": "^0.30.8",
    "gulp": "^4.0.2",
    "gulp-file-include": "^2.3.0",
    "gulp-format-html": "^1.2.5",
    "gulp-sass": "^5.0.0"
  }
}
